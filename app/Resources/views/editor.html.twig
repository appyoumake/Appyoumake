<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
        	<link href="{{ asset("/css/temp.css") }}" rel="stylesheet" />
			<link href="{{ asset("/css/jquery-ui.css") }}" rel="stylesheet" />
			<link href="{{ asset("/css/jquery.qtip.css") }}" rel="stylesheet" />
			<link href="{{ asset("/css/jquery.uploadfile.css") }}" rel="stylesheet" />
			<link href="{{ asset("/css/jquery.megamenu.css") }}" rel="stylesheet" />
            <link href="{{ asset("/css/ui.dropdownchecklist.standalone.css") }}" rel="stylesheet" />
			<link href="{{ asset("/css/mlab_all.css") }}" rel="stylesheet" />
			<link href="{{ asset("/css/mlab_editor.css") }}" rel="stylesheet" />
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    
    <body id="mlab_editor">
    	<div class="mlab_min_page_width">
    		<div class="mlab_align_menu_elements">
	    		{% if app.user %}
	    			{{ render(controller('SinettMLABBuilderBundle:Menu:buildMenu', {'path': path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) } )) }}
		    	{% endif %}
	    	<div class="container">
	 			{% block body %}{% endblock %}
	 		</div>
	        {% block javascripts %}
	        	<script src="{{ asset('/js/jquery-2.0.3.js') }}"></script>
	        	<script src="{{ asset('/js/jquery-ui.js') }}"></script>
	        	<script src="{{ asset('/js/jquery.uploadfile.js') }}"></script>
	        	<script src="{{ asset('/js/jquery.qtip.js') }}"></script>
	        	<script src="{{ asset('/js/jquery.megamenu.js') }}"></script>  
	        	<script src="{{ asset('/js/ddslick.js') }}"></script>
                <script src="{{ asset('/js/ui.dropdownchecklist-1.4-min.js') }}"></script>
                
		        <script type="text/javascript">	 
			        $(document).ready(function() {
			        	jQuery("#mlab_megamenu").megamenu();
			        });
			    </script>
	        {% endblock %}
	        </div>
	        <div class="mlab_top_bar_back"></div>
	    </div>
        <h1 contenteditable="true">Test edit</h1>
    </body>
</html>
