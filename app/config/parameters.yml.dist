# This file is used by the Mlab installation file as a starting point for creating a configuration file.
# Entries prefixed with ___ (triple underscore) are those that need to be edited for the local installation
parameters:
    ___database_driver: pdo_mysql
    ___database_host: 127.0.0.1
    ___database_port: null
    ___database_name: 
    ___database_user: 
    ___database_password: 
    ___mailer_transport: smtp
    ___mailer_host: 127.0.0.1
    ___mailer_user: null
    ___mailer_password: null
    ___locale: nb_NO
    supported_locales: {en_GB: 'English', nb_NO: 'Norsk'}
    secret: 
    security.authentication.success_handler.class: Sinett\MLAB\BuilderBundle\Handler\AuthenticationSuccessHandler
    
    mlab: 
        convert:
            ___python_bin: 
            converter_bin: document2HTML.py
            config: config.json
            converter_path: src/Sinett/MLAB/BuilderBundle/FileManagement/conv/

        ws_socket:
            ___url_client: ws://url:8080/
            path_client: /messages/
#must be in this format
            ___url_server: http://url:8080/
            path_server: /messages/

        missing_icon: data:image/png;base64,
        save_interval: 60
        icon_text_maxlength: 6

        uploads_allowed:
          ___img: [image/gif, image/jpeg, image/png]
          ___video: [video/webm, video/mp4, video/ogg]
          ___audio: [audio/mp4, audio/mpeg, audio/vnd.wave]
          
        paths:
          ___app: 
          ___component: 
          ___template: 
          ___icon: 
        
        urls:
          ___app: 
          ___component: 
          ___template: 
          ___icon: 
          
        filenames:
          component_icon: icon.png
          app_icon: icon.png
          app_splash_screen: splash
          app_config: conf.json
        
        compiler_service:
            ___supported_platforms: [ios, android]
            default_icon: data:image/png;base64,
            ___url: 
            ___protocol: http
            ___passphrase: 
            config_uid_tag: widget
            config_uid_attribute: id
            ___app_creator_identifier: com.test.apps
            app_uid_metatag_placeholder: %%%%APP_UID%%%%
            ___target_version: {ios: 7, android: 18}
            file_extensions: {ios: 'ipa', android: 'apk'}
            ___rsync_bin: 
            ___rsync_url: user@url::cs_inbox
            ___rsync_password: 
            rsync_suffix: /www/


# when a user is uploading a file these files have to be present to be accepted
        verify_uploads:
            template:
                - frontpage.html
                - page.html
                - thumbnail.jpg
                - conf.yml
                - js/jquery.js
                - js/jquery-ui.js
                - js/jquery-mobile.js
                - js/mlab.api.js
                - css/mlab_component.css
                - css/jquery-ui.css
                - css/jquery-mobile.css
            template_conf_required: [name, tooltip]
            component:
                - code_dt.js
                - conf.yml
                - icon.png
            component_conf_required: [category, html, name, tooltip]
            app:
                - www
            template_required_strings:
                frontpage.html: '/\<div .*id\=.mlab_editable_area.*?\>/'
            
        component_files: { ICON: icon.png, CONFIG: conf.yml, SCRIPTS: code_dt.js, PHP: server_code.php }
        
        app:
            copy_files: { frontpage.html: index.html, js/: js, css/: css, img/: img }
            new_page: page.html
            content_id: mlab_editable_area
            html_header: <!DOCTYPE html>\n<html>\n<head><title>%%TITLE%%</title></head>\n<body>\n
            html_footer: \n</body></html>

            
        replace_in_filenames: { a: '/[ÂÃÄÀÁÅàáâãäå]/', ae: '/[Ææ]/', c: '/[Çç]/', e: '/[ÈÉÊËèéêë]/', i: '/[ÌÍÎÏìíîï]/', o: '/[ÒÓÔÕÖØòóôõöø]/', n: '/[Ññ]/', u: '/[ÙÚÛÜùúûü]/', y: '/[Ýýÿ]/', _: '/[^A-Za-z0-9]/' }    

        plugins: ["cordova-plugin-device", "cordova-plugin-network-information"]
