{# Insert menu tools #}
<div id="mlab_toolbar_components">
    <button data-action-click="newPage" class="toolbox-btn btn-lg" data-tooltip="{% trans %}editor.tabs.file.save.tooltip{% endtrans %}">
        <span class="badge badge-top-right badge-plus">+</span>
        <i class="far fa-file"></i> {% trans %}editor.tabs.file.new.page{% endtrans %}
    </button>
    <button data-action-click="newSection" class="toolbox-btn btn-lg" data-tooltip="{% trans %}editor.tabs.file.save.tooltip{% endtrans %}">
        <span class="badge badge-top-right badge-plus">+</span>
        <i class="fas fa-book-open"></i> {% trans %}editor.tabs.file.new.page{% endtrans %}
    </button>
    
    <div class="v-separator"></div>

</div>



