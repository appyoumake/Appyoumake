<tr id="row_group_{{ entity.id }}" class="row_activ_{{ entity.enabled }}">

{% if entity.roles %}
	{% if entity.roles[0] == "ROLE_SUPER_ADMIN" %}
   		<td class="mlab_icon_role_super_admin" title="{% trans %}app.admin.users.tooltip.username{% endtrans %}: {{ entity.username }}">
	{% elseif entity.roles[0] == "ROLE_ADMIN" %}
		<td class="mlab_icon_role_admin" title="{% trans %}app.admin.users.tooltip.username{% endtrans %}: {{ entity.username }}">
	{% else %}
    	<td class="mlab_icon_role_rest" title="{% trans %}app.admin.users.tooltip.username{% endtrans %}: {{ entity.username }}">
    {% endif %}
    	{% else %}
    	<td class="mlab_icon_role_none" title="{% trans %}app.admin.users.tooltip.username{% endtrans %}: {{ entity.username }}">
    {% endif %}
      
    	<a href="{{ path('user_edit', { 'id': entity.id }) }}">
    		{{ entity.username }}
    	</a>
    </td>

    
    <td>{{ entity.email }}</td>
    
    <td> {{ entity.groups|join(', ') }} </td>
    
    {% if entity.lastLogin %}
    	<td title="{{ entity.lastLogin|date('Y-m-d H:i:s') }}">{{ entity.lastLogin|date('Y-m-d') }}</td>
    {% else %}
    	<td>-</td>
    {% endif %}
    
    <td>
    	<a href="{{ path('user_delete', { 'id': entity.id }) }}" class="mlab_icon_delete" title="{% trans %}app.admin.users.tooltip.delete{% endtrans %}: {{ entity.username }}"></a>
    </td>
</tr>
